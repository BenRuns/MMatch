        <div class="row placeholders">
          <div class="col-xs-6 col-sm-2 placeholder">
            <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="Generic placeholder thumbnail">
            <%= label_tag(:laser_cutter, "Laser Cutter") %>
            <%= check_box_tag(:laser_cutter) %>
          </div>
          <div class="col-xs-6 col-sm-2 placeholder">
            <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="Generic placeholder thumbnail">
            <%= label_tag(:three_d_printer, "3D Printer") %>
            <%= check_box_tag(:three_d_printer) %>
          </div>
          <div class="col-xs-6 col-sm-2 placeholder">
            <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="Generic placeholder thumbnail">
            <%= label_tag(:cnc_router, "CNC Router") %>
            <%= check_box_tag(:cnc_router) %>
          </div>
          <div class="col-xs-6 col-sm-2 placeholder">
            <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="Generic placeholder thumbnail">
            <%= label_tag(:sewing_maching, "Sewing Machine") %>
            <%= check_box_tag(:sewing_maching) %>
          </div>
          <div class="col-xs-6 col-sm-2 placeholder">
            <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="Generic placeholder thumbnail">
            <%= label_tag(:microcontroller_library, "Microcontroller Lib") %>
            <%= check_box_tag(:microcontroller_library) %>
          </div>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <div class="col-xs-6 col-sm-3 placeholder">
            <%= label_tag(:q, "State:") %>
            <%= text_field_tag(:state) %>
          </div>
          <div class="col-xs-6 col-sm-3 placeholder">
            <%= label_tag(:q, "City:") %>
            <%= text_field_tag(:city) %>
          </div>
          <div class="col-xs-6 col-sm-3 placeholder">
            <br />
            <%= submit_tag("Search") %>
          </div>
        </div>
        <% end %>

